[credential "https://github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[credential "https://gist.github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[user]
	email = 47441022+NathanSnail@users.noreply.github.com
	name = Nathan
[init]
	defaultBranch = main
[push]
	recurseSubmodules = on-demand
[submodule]
	recurse = true
[core]
	attributesfile = ~/.gitattributes
	autocrlf = false
	eol = lf
	editor = nvim
[alias]
	diff-image = "!f() { cd -- \"${GIT_PREFIX:-.}\"; GIT_DIFF_IMAGE_ENABLED=1 git diff \"$@\"; }; f"
[diff "image"]
	command = /home/nathan/bin/diff-image
[rerere]
	enabled = true
[column]
	ui = auto
[branch]
	sort = -committerdate
[pull]
	rebase = false
